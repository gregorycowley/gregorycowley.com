{
  "rewrites": [
    {
      "source": "/articles",
      "destination": "https://gregorycowley-com-articles.vercel.app/articles"
    },
    {
      "source": "/articles/:path*",
      "destination": "https://gregorycowley-com-articles.vercel.app/articles/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}