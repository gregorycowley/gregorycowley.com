{
  "outputDirectory": "apps/portfolio/.next",
  "rewrites": [
    {
      "source": "/articles",
      "destination": "https://gregorycowley-com-articles.vercel.app/articles"
    },
    {
      "source": "/articles/:path*",
      "destination": "https://gregorycowley-com-articles.vercel.app/articles/:path*"
    },
    {
      "source": "/case-studies",
      "destination": "https://gregorycowley-com-notion.vercel.app/case-studies"
    },
    {
      "source": "/cca-application",
      "destination": "https://gregorycowley-com-notion.vercel.app/case-studies"
    },
    {
      "source": "/case-studies/:path*",
      "destination": "https://gregorycowley-com-notion.vercel.app/case-studies/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}