{
  "name": "gregorycowley-com",
  "version": "0.9.2",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "articles",
    "portfolio",
    "packages/*"
  ],
  "scripts": {
    "portfolio": "yarn --cwd portfolio run start",
    "portfolio:dev": "yarn --cwd portfolio run dev",
    "portfolio:build": "yarn --cwd portfolio run build",
    "articles": "yarn --cwd articles run start",
    "articles:dev": "yarn --cwd articles run dev",
    "articles:build": "yarn --cwd articles run build",
    "all:install": "yarn --cwd portfolio install && yarn --cwd articles install",
    "all:build": "npm-run-all --parallel articles:build portfolio:build",
    "all:dev": "npm-run-all --parallel articles:dev portfolio:dev"
  },
  "devDependencies": {
    "debug": "^4.3.4",
    "npm-run-all": "^4.1.5"
  }
}
